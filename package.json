{
  "name": "tpo-apin",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/db/data-source.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/db/data-source.ts",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show -d src/db/data-source.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/db/data-source.ts src/db/migrations/AutoMigration",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create src/db/migrations/ManualMigration"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@types/yamljs": "^0.2.34",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26",
    "uuid": "^9.0.1",
    "yamljs": "^0.3.0"
  }
}